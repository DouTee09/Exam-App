<div class = "left-box-sub">
    <%= form_with(model: @subject, local: true) do |f| %>
        <div class="form-group">
            <%= render "shared/error_messages", object: @subject %>
            <label>Thêm môn học mới</label>
            <div class="col-sm-10">
                <%= f.text_field :name, class: "form-control", placeholder: "Tên môn học" %>
            </div>
        </div>
            <div class="col-sm-10">
            <%= f.submit "Create subject", class: "btn btn-primary" %>
        </div>
    <% end %>
</div>
